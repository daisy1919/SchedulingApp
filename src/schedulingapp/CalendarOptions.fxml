<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="155.0" prefWidth="200.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="schedulingapp.CalendarOptionsController">
    <children>
      <Label layoutX="30.0" layoutY="15.0" prefHeight="20.0" prefWidth="140.0" text="Select an option below.">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="weekApptsButton" layoutX="30.0" layoutY="50.0" mnemonicParsing="false" onAction="#handleWeekButton" prefHeight="30.0" prefWidth="140.0" text="View Week" />
      <Button fx:id="editAppointmentButton" layoutX="30.0" layoutY="95.0" mnemonicParsing="false" onAction="#handleMonthButton" prefHeight="30.0" prefWidth="140.0" text="View Month" />
    </children>
</AnchorPane>