<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="380.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="schedulingapp.OnLoginController">
   <children>
      <Label layoutX="33.0" layoutY="30.0" prefWidth="185.0" text="Select an option below to begin">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="customerButton" layoutX="60.0" layoutY="70.0" mnemonicParsing="false" onAction="#handleCustomerButton" prefHeight="50.0" prefWidth="130.0" text="Customer Records" />
      <Button fx:id="appointmentButton" layoutX="60.0" layoutY="140.0" mnemonicParsing="false" onAction="#handleAppointmentButton" prefHeight="50.0" prefWidth="130.0" text="Appointments" />
      <Button fx:id="calendarButton" layoutX="60.0" layoutY="210.0" mnemonicParsing="false" onAction="#handleCalendarButton" prefHeight="50.0" prefWidth="130.0" text="Calendar" />
      <Button fx:id="reportsButton" layoutX="60.0" layoutY="280.0" mnemonicParsing="false" onAction="#handleReportsButton" prefHeight="50.0" prefWidth="130.0" text="Reports" />
   </children>
</AnchorPane>
