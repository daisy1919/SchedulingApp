<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="260.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="schedulingapp.ReportOptionsController">
    <children>
      <Label layoutX="67.0" layoutY="15.0" prefHeight="20.0" prefWidth="135.0" text="Select an report below.">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="apptTypeButton" layoutX="30.0" layoutY="50.0" mnemonicParsing="false" onAction="#handleApptTypeButton" prefHeight="30.0" prefWidth="200.0" text="Appointment Types by Month" />
      <Button fx:id="consultantButton" layoutX="30.0" layoutY="95.0" mnemonicParsing="false" onAction="#handleConsultantButton" prefHeight="30.0" prefWidth="200.0" text="Consultant Schedule" />
      <Button fx:id="additionalReportButton" layoutX="30.0" layoutY="140.0" mnemonicParsing="false" onAction="#handleAdditionalReportButton" prefHeight="30.0" prefWidth="200.0" text="**FINISH ME**" />
    </children>
</AnchorPane>
