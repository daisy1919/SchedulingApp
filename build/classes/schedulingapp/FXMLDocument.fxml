<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="170.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="schedulingapp.FXMLDocumentController">
    <children>
      <Label layoutX="92.5" layoutY="18.0" prefHeight="18.0" prefWidth="115.0" text="Please log in below">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <Label layoutX="30.0" layoutY="50.0" prefHeight="17.0" prefWidth="69.0" text="Username: " />
      <TextField fx:id="usernameText" layoutX="100.0" layoutY="48.0" />
      <Label layoutX="30.0" layoutY="80.0" text="Password: " />
      <TextField fx:id="passwordText" layoutX="100.0" layoutY="78.0" />
      <Button fx:id="submitButton" layoutX="175.0" layoutY="120.0" mnemonicParsing="false" onAction="#handleLoginButton" prefHeight="25.0" prefWidth="65.0" text="Log In" />
      <Button fx:id="exitButton" layoutX="70.0" layoutY="120.0" mnemonicParsing="false" onAction="#handleExitButton" prefHeight="25.0" prefWidth="65.0" text="Exit" />
      <Label fx:id="errorMessages" layoutX="10.0" layoutY="148.0" prefWidth="280.0" textAlignment="CENTER" textFill="RED" />
    </children>
</AnchorPane>
